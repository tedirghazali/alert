{
  "name": "tedir-alert",
  "version": "0.0.1-alpha-2",
  "description": "Tedir Alert is a custom alert or notification for Vue 3 & Web Component",
  "main": "dist/vue/tedir-alert-vue.umd.js",
  "module": "dist/vue/tedir-alert-vue.es.js",
  "unpkg": "dist/vue/tedir-alert-vue.umd.js",
  "exports": {
    ".": {
      "import": "./dist/vue/tedir-alert-vue.es.js",
      "require": "./dist/vue/tedir-alert-vue.umd.js"
    },
    "./web": {
      "import": "./dist/web/tedir-alert-web.es.js",
      "require": "./dist/web/tedir-alert-web.umd.js"
    },
    "./css": "./dist/vue/style.css"
  },
  "scripts": {
    "dev-vue": "vite --config vite-vue-config.ts",
    "build-vue": "vue-tsc --noEmit && vite build --config vite-vue-config.ts",
    "build-web": "vue-tsc --noEmit && vite build --config vite-web-config.ts",
    "preview-vue": "vite preview --config vite-vue-config.ts"
  },
  "files": [
    "src/alga/*.alga",
    "package.alga",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tedirghazali/slider.git"
  },
  "keywords": [
    "slider",
    "slideshow",
    "carousel"
  ],
  "author": "Tedir Ghazali",
  "license": "SEE LICENSE IN license.txt",
  "bugs": {
    "url": "https://github.com/tedirghazali/slider/issues"
  },
  "homepage": "https://github.com/tedirghazali/slider#readme",
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "alga-css": "^1.0.0-beta-51",
    "alga-js": "^0.1.0-beta-17",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.24"
  }
}
